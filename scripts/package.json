{
  "name": "doramaflix-migration-scripts",
  "version": "1.0.0",
  "description": "Migration scripts for DoramaFlix infrastructure",
  "type": "module",
  "main": "migrate-vercel-to-spaces.js",
  "scripts": {
    "backup": "node pre-migration-backup.js",
    "migrate": "node migrate-vercel-to-spaces.js",
    "migrate:dry-run": "node migrate-vercel-to-spaces.js --dry-run",
    "migrate:videos": "node migrate-vercel-to-spaces.js --file-type video",
    "migrate:images": "node migrate-vercel-to-spaces.js --file-type image",
    "migrate:test": "node migrate-vercel-to-spaces.js --dry-run --max-files 10",
    "verify": "node post-migration-verify.js",
    "verify:performance": "node post-migration-verify.js --check-performance",
    "verify:sample": "node post-migration-verify.js --sample-size 50",
    "full-migration": "npm run backup && npm run migrate && npm run verify"
  },
  "keywords": [
    "migration",
    "vercel-blob",
    "digitalocean-spaces",
    "doramaflix"
  ],
  "author": "DoramaFlix Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "@opentf/cli-pbar": "^2.1.0",
    "@vercel/blob": "^0.15.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "p-limit": "^4.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}